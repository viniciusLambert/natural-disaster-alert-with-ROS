<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="mainwindow">
    <property name="borderwidth">20</property>
    <property name="height">200</property>
    <property name="takefocus">false</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="title">
        <property name="anchor">w</property>
        <property name="font">TkDefaultFont</property>
        <property name="justify">center</property>
        <property name="padding">20</property>
        <property name="text" translatable="yes">Sistema de Previsão
Publisher</property>
        <property name="underline">0</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="central_button">
        <property name="command">registrar_polo</property>
        <property name="default">normal</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Criar central de aviso</property>
        <property name="underline">0</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="display">
        <property name="text" translatable="yes">Display 
</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="quit">
        <property name="command">quit_on_click_button</property>
        <property name="cursor">arrow</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Sair</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">15</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="lbl_local">
        <property name="text" translatable="yes">Entre com um local</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="lbl_tipo">
        <property name="justify">center</property>
        <property name="text" translatable="yes">Tipo de desastre</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="lbl_dia">
        <property name="justify">center</property>
        <property name="text" translatable="yes">Dia (DD)</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="lbl_mes">
        <property name="justify">center</property>
        <property name="text" translatable="yes">Mês (MM)</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="lbl_ano">
        <property name="justify">center</property>
        <property name="text" translatable="yes">Ano (AA)</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">8</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="lbl_hora">
        <property name="justify">center</property>
        <property name="text" translatable="yes">Hora (HH:MM)</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">9</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="criar_aviso">
        <property name="command">envia_aviso</property>
        <property name="text" translatable="yes">Criar aviso</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">10</property>
          <property name="propagate">True</property>
          <property name="row">11</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="display2">
        <property name="text" translatable="yes">Display 2</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">12</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
